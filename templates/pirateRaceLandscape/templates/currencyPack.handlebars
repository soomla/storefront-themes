<div class="content font-face-2" data-categoryid="{{currency_itemId}}" style="background-image: url({{backgroundImgFilePath}})">
  <div class="row">
    <div class="description">
      <label style="{{nameStyle}}">{{name}}</label>
    </div>
  </div>
  <div class="row balanceRow">
    <img class="visual" src="{{imgFilePath}}" />
  </div>
  <div class="row price">
    <label style="{{priceStyle}}">{{price}}$</label>
    <div class="currency"></div>
    <a class="buy">
      <img src="{{buy}}" />
    </a>
  </div>
</div>